{
  "name": "blachere/space",
  "type": "project",
  "description": "",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "5.0.*",
    "doctrine/collections": "v1.6.7"
  },
  "require-dev": {
    "phpunit/phpunit": "8.3",
    "symfony/phpunit-bridge": "^5.0",
    "phpstan/phpstan": "0.12.54",
    "squizlabs/php_codesniffer": "3.5.8"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/src"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.1.*"
    }
  }
}
